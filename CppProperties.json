        {
            "inheritEnvironments": ["msvc_x64"],
            "name": "x64-Debug",
            "includePath": [
                "${env.INCLUDE}",
                "${workspaceRoot}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "YASHIMA_DEBUG",
                "YASHIMA_PLATFORM_WINDOWS"
            ],
            "intelliSenseMode": "windows-msvc-x64"
        },
        {
            "inheritEnvironments": ["msvc_x64"],
            "name": "x64-Release",
            "includePath": [
                "${env.INCLUDE}",
                "${workspaceRoot}/**"
            ],
            "defines": [
                "NDEBUG",
                "UNICODE",
                "_UNICODE",
                "YASHIMA_RELEASE",
                "YASHIMA_PLATFORM_WINDOWS"
            ],
            "intelliSenseMode": "windows-msvc-x64"
        }
    ]
}